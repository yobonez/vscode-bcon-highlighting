{
  "name": "bcon-syntax-highlighting",
  "displayName": "BCON Syntax highlighting",
  "description": "Syntax highlighting support for BCON.",
  "icon": "icon.png",
  "version": "1.0.1",
  "license": "MIT",
  "publisher": "yobonez",
  "author": "yobonez <59935203+yobonez@users.noreply.github.com> (https://github.com/yobonez/)",
  "contributors": ["ParrotCore"],
  "repository": {
    "type": "git",
    "url": "https://github.com/yobonez/vscode-bcon-highlighting.git"
  },
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "bcon",
      "aliases": ["BCON Configures Only Nicely", "bcon"],
      "extensions": [".bcon"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "bcon",
      "scopeName": "source.bcon",
      "path": "./syntaxes/bcon.tmLanguage.json"
    }
    ]
  }
}
